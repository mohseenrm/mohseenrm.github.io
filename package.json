{
  "name": "mohseenrm.github.io",
  "version": "2.0.0",
  "description": "personal website",
  "main": "index.html",
  "scripts": {
    "build": "gulp build",
    "build:watch": "babel -w js/ -d dist/ -s",
    "build:webpack": "\"./node_modules/.bin/webpack\"",
    "build:webpack:production": "\"./node_modules/.bin/webpack\" -p",
    "devServer": "./node_modules/.bin/webpack-dev-server --progress --colors",
    "compile": "sass --watch src/scss:dist/css",
    "build:production": "gulp",
    "test": "node_modules/.bin/eslint src/js/*.js --cache --cache-location ./.eslintcache",
    "test:quick-fix": "node_modules/.bin/eslint src/js/*.js --cache --cache-location ./.eslintcache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:mohseenrm/mohseenrm.github.io.git"
  },
  "keywords": [
    "website"
  ],
  "author": "Mohseen Mukaddam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohseenrm/mohseenrm.github.io/issues"
  },
  "homepage": "https://mohseenrm.github.io",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-bump": "^2.5.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^2.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-uglify": "^2.0.0",
    "gulp-uglifycss": "^1.0.8",
    "gulp-util": "^3.0.8",
    "postcss-loader": "^2.0.6",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {}
}
